<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.varun.preference.page_final.EASEPreferences"
            id="com.varun.preference.page.final.pageEASE1"
            name="EASE">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="EASE">
            <command
                  commandId="com.varun.preference.page.final.command1"
                  label="ShowPreferences"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.varun.preference.handler.ShowPrefs"
            id="com.varun.preference.page.final.command1"
            name="SeePreferences">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.varun.preference.handler.ShowPrefs"
            commandId="com.varun.preference.page.final.handler1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.varun.preference.page_final.Initializer">
      </initializer>
   </extension>

</plugin>
